			<rule indicationColor="HIGH">
				<condition type="getDataAsDouble" param="ADYSY" value="&gt;=4" />
			</rule>
		</ruleset>
	</item>
	<item measurement_type="ACOSY" display_name="cough" guideline="Target: &lt;4 days/week" graphable="yes" value_name="cough">
