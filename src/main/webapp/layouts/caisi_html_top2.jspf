<%
	ArrayList<String> errors=WebUtils.popErrorMessages(session);
	if (errors!=null)
	{
		%>
			<ul style="color:red">
				<%
					for (String error : errors)
					{
						%>
							<li><%=StringEscapeUtils.escapeHtml(error)%></li>
						<%
					}
				%>
			</ul>
